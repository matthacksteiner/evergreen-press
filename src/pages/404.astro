---
import { getIndex } from '@lib/api.js';
import Layout from '@layouts/Layout.astro';
import Blocks from '@components/Blocks.astro';

const data = await getIndex(`/z-error.json`);
const global = data.global;
const pageTitle = global.siteTitle + ' | ' + data.title;
---

<Layout pageTitle={pageTitle} data={data} global={global}>
	<Blocks blocks={data.blocks} global={global} />
</Layout>
