---
interface Props {
	srcsetMobile: string;
	srcset: string;
	src: string;
	alt: string;
	width: string;
	height: string;
}
const {
	src,
	srcsetMobile,
	srcset,
	class: className,
	...pictureProps
} = Astro.props;
---

<picture>
	<source srcset={srcsetMobile} media="(max-width: 768px)" type="image/webp" />
	<source srcset={srcset} media="(min-width: 769px)" type="image/webp" />
	<img src={src} class={className} {...pictureProps} />
</picture>
