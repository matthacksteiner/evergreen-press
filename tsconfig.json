{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "astro/client"
    ],
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@blocks/*": [
        "src/blocks/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@scripts/*": [
        "src/scripts/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ]
    }
  }
}